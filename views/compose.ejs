<%- include("partials/header")%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label >Title</label>
        <input type="text"  class="form-control"  name="title" value="<%=head%>">
        <label >Post</label>
        <textarea name="postbody" cols="30" rows="10"class="form-control" ><%=body%></textarea>
        </div>
    <button type="submit" class="btn  btn-primary" name="comp"value="<%=Id%>">Publish</button>
</form>
<%-include("partials/footer")%>